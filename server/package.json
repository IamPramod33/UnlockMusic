{
  "name": "@unlockmusic/server",
  "version": "0.1.0",
  "description": "Backend API for Unlock Music Learning",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npm run seed",
    "db:studio": "npx prisma studio",
    "setup": "npm install && echo 'DATABASE_URL=\"file:./dev.db\"' > .env && npx prisma migrate dev --name init --skip-seed && npm run seed",
    "start:dev": "npm run setup && npm run dev"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.9",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "private": true
}
